{
  "name": "manhattan-dashboard-optimized",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prebuild": "echo 'Starting optimized build with' $(node --version) && echo 'Using pnpm' $(pnpm --version)",
    "build": "next build",
    "postbuild": "echo 'Build complete. Bundle size:' && du -sh .next",
    "build:analyze": "ANALYZE=true next build",
    "clean": "rm -rf .next node_modules",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "dev": "next dev --turbo",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "monitor:performance": "node scripts/performance-monitor.js",
    "perf:analyze": "pnpm build:analyze && open bundle-analysis.html",
    "perf:lighthouse": "lighthouse http://localhost:3000 --output=html --output-path=./lighthouse-report.html",
    "precommit": "pnpm lint && pnpm type-check",
    "reinstall": "rm -rf node_modules pnpm-lock.yaml && pnpm install",
    "start": "next start",
    "sync:properties": "node scripts/sync-properties.js",
    "test:build": "pnpm build && pnpm start",
    "type-check": "tsc --noEmit",
    "vercel-build": "pnpm install --no-frozen-lockfile && pnpm build"
  },
  "dependencies": {
    "@neondatabase/serverless": "latest",
    "@radix-ui/react-accordion": "1.2.3",
    "@radix-ui/react-alert-dialog": "1.1.4",
    "@radix-ui/react-avatar": "1.1.3",
    "@radix-ui/react-badge": "1.1.3",
    "@radix-ui/react-button": "1.1.3",
    "@radix-ui/react-card": "1.1.3",
    "@radix-ui/react-dropdown-menu": "2.1.6",
    "@radix-ui/react-progress": "latest",
    "@radix-ui/react-select": "latest",
    "@radix-ui/react-slot": "latest",
    "@radix-ui/react-switch": "1.2.6",
    "@radix-ui/react-tabs": "1.1.13",
    "@upstash/redis": "latest",
    "@vercel/analytics": "1.5.0",
    "ai": "latest",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "framer-motion": "12.23.21",
    "fs": "latest",
    "https": "latest",
    "lucide-react": "^0.454.0",
    "next": "15.2.4",
    "next-themes": "latest",
    "path": "latest",
    "perf_hooks": "latest",
    "react": "^19",
    "react-dom": "^19",
    "recharts": "latest",
    "server-only": "latest",
    "tailwind-merge": "^2.5.5",
    "tailwindcss-animate": "^1.0.7",
    "vaul": "1.1.2",
    "zod": "4.1.11",
    "zustand": "5.0.8"
  },
  "devDependencies": {
    "@types/node": "^22",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "critters": "^0.0.24",
    "eslint-config-next": "^15.5.4",
    "lighthouse": "^12.0.0",
    "postcss": "^8.5",
    "tailwindcss": "^4.1.9",
    "typescript": "^5",
    "webpack-bundle-analyzer": "^4.10.1"
  },
  "packageManager": "pnpm@10.17.1",
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=10.0.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "allowedVersions": {
        "react": "19",
        "react-dom": "19"
      },
      "ignoreMissing": [
        "react",
        "react-dom"
      ]
    },
    "overrides": {
      "recharts": {
        "react": "19.1.1",
        "react-dom": "19.1.1"
      },
      "vaul": {
        "react": "19.1.1",
        "react-dom": "19.1.1"
      }
    },
    "approvedBuilds": [
      "@tailwindcss/oxide"
    ]
  }
}